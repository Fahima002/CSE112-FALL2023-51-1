#include <iostream>
using namespace std;
class ManchesterUnited {
private:
    int coach;
    int player;

public:
    ManchesterUnited() : coach(5), player(6) {}

    ManchesterUnited(int c, int p) : coach(c), player(p) {}

    void getData() {
        std::cout << "Coach: " << coach << ", Player: " << player << std::endl;
    }

    ManchesterUnited operator++(int) {
        ManchesterUnited temp = *this;
        coach++;
        player++;
        return temp;
    }

    bool operator==(const ManchesterUnited& other) {
        return (coach == other.coach) && (player == other.player);
    }
};

int main() {
    ManchesterUnited ronaldo;
    ManchesterUnited fernandes(5, 6);

    ronaldo++;
    ronaldo.getData();
    fernandes.getData();

    if (ronaldo == fernandes) {
        std::cout << "Ronaldo and Fernandes are equal." << std::endl;
    } else {
        std::cout << "Ronaldo and Fernandes are not equal." << std::endl;
    }

    return 0;
}
